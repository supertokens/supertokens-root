{
  "name": "supertokens-random-app",
  "template": {
    "firstFactors": [
      "emailpassword",
      "thirdparty"
    ],
    "providers": [
      "google",
      "github"
    ]
  },
  "strategy": "docker",
  "items": [
    {
      "id": "create-supertokens-app",
      "target": "create-supertokens-app",
      "scripts": {
        "build": "npm run build-pretty"
      }
    },
    {
      "id": "supertokens-web-js",
      "target": "supertokens-web-js",
      "scripts": {
        "build": "npm run build-pretty"
      }
    },
    {
      "id": "supertokens-auth-react",
      "target": "supertokens-auth-react",
      "libs": [
        "supertokens-web-js"
      ],
      "scripts": {
        "build": "npm run build-pretty"
      }
    },
    {
      "id": "dashboard",
      "target": "dashboard",
      "scripts": {
        "start": "export PORT=$ST_PORT && npm run start-bundle-server"
      }
    },
    {
      "id": "docs",
      "target": "docs",
      "scripts": {
        "start": "npm run start -- --port $ST_PORT"
      }
    },
    {
      "id": "supertokens-core",
      "target": "supertokens-core",
      "scripts": {
        "start": "cd ../../ && ./scripts/core/startTestEnv --wait | while read line; do echo \"$line\"; [[ \"$line\" == *\"Test environment started! Leave this script running while running tests.\"* ]] && ./scripts/core/runCore $ST_PORT; done"
      }
    },
    {
      "id": "react",
      "libs": [
        "supertokens-auth-react"
      ],
      "target": "react",
      "scripts": {
        "start": "npm run start",
        "test": "npm run test"
      }
    },
    {
      "id": "supertokens-node",
      "target": "express",
      "libs": [
        "supertokens-node"
      ],
      "scripts": {
        "start": "npm run start"
      }
    }
  ]
}